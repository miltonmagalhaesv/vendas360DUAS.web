  "tableData": "{{\n  (() => {\n    const data = qry_AtingimentoMensal.data;\n    const meses = [\"Jan\", \"Fev\", \"Mar\", \"Abr\", \"Mai\", \"Jun\", \"Jul\", \"Ago\", \"Set\", \"Out\", \"Nov\", \"Dez\"];\n    \n    // Inicializa as linhas da tabela\n    const faturado = { \"Indicador\": \"Faturado\" };\n    const meta = { \"Indicador\": \"Meta\" };\n    const diferenca = { \"Indicador\": \"DiferenÃ§a\" };\n\n    let totalFaturado = 0;\n    let totalMeta = 0;\n\n    data.forEach(item => {\n      const mesIndex = parseInt(item.mes_ano.split('-')[1], 10) - 1;\n      const nomeMes = meses[mesIndex];\n      \n      faturado[nomeMes] = item.faturamento_liquido.toLocaleString('pt-BR', { style: 'currency', currency: 'BRL' });\n      meta[nomeMes] = item.meta_valor.toLocaleString('pt-BR', { style: 'currency', currency: 'BRL' });\n      \n      const diffValor = item.faturamento_liquido - item.meta_valor;\n      diferenca[nomeMes] = `${diffValor.toLocaleString('pt-BR', { style: 'currency', currency: 'BRL' })} (${item.perc_atingimento}%)`;\n\n      totalFaturado += item.faturamento_liquido;\n      totalMeta += item.meta_valor;\n    });\n\n    // Adiciona a coluna Total\n    faturado[\"Total\"] = totalFaturado.toLocaleString('pt-BR', { style: 'currency', currency: 'BRL' });\n    meta[\"Total\"] = totalMeta.toLocaleString('pt-BR', { style: 'currency', currency: 'BRL' });\n    const totalDiff = totalFaturado - totalMeta;\n    const totalPerc = totalMeta > 0 ? ((totalFaturado * 100) / totalMeta).toFixed(2) : 0;\n    diferenca[\"Total\"] = `${totalDiff.toLocaleString('pt-BR', { style: 'currency', currency: 'BRL' })} (${totalPerc}%)`;\n\n    return [faturado, meta, diferenca];\n  })()\n}}\n",
